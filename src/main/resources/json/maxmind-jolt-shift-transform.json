      "code": "continent.code"
    },
    "country": {
      "names": { "en": "country.name" },
      "iso_code": "country.iso_code"
    },
    "location": {
      "accuracy_radius": "location.accuracy_radius",
      "latitude": "location.latitude",
      "longitude": "location.longitude",
      "metro_code": "location.metro_code",
      "time_zone": "location.time_zone"
    },
    "postal": {
      "code": "postal.code"
    },
    "registered_country": {
      "names": { "en": "registered_country.name" },
      "iso_code": "registered_country.iso_code"
    },
    "represented_country": {
      "names": { "en": "represented_country.name" },
      "iso_code": "represented_country.iso_code"
    },
    "subdivisions": {
      "*": {
        "iso_code": "subdivisions[&1].iso_code",
